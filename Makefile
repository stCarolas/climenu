install:
	cp m1.py /usr/local/bin
	cp Examples/tmux_menu.py /Users/stCarolas/.config/m1/generators
	cp Examples/menu /Users/stCarolas/.config/m1/

install_to_home:
	cp m1.py ~/.local/bin/m1
	cp Examples/tmux_menu.py /Users/stCarolas/.config/m1/generators
	cp Examples/menu /Users/stCarolas/.config/m1/
